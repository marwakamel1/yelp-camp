<%- include("../partials/header.ejs") %>
<form action="/campgrounds/<%=finded._id%>?_method=PUT" method="POST">
  <div class="form-group">
    <input type="text" name="edited[name]" class="form-control"  placeholder="name" value="<%= finded.name%>">
  </div>
  <div class="form-group">
    <input type="text" name="edited[image]" class="form-control"  placeholder="image url" value="<%= finded.image%>">
  </div>
  <div class="form-group">
    <input type="number" name="edited[price]" class="form-control"  placeholder="price" value="<%= finded.price%>">
  </div>
  <div class="form-group">
    <textarea   name="edited[description]" class="form-control"  rows="3" ><%= finded.description%></textarea>
  </div>

  <button type="submit" class="btn btn-default">Submit</button>
</form>


<%- include("../partials/footer.ejs") %>